{
  "name": "fullstackwiki-ci",
  "version": "0.0.0",
  "description": "Publishes changes to Fullstack.wiki",
  "author": "Austin Wright <aaa@bzfx.net>",
  "license": "Unlicense",
  "keywords": [
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
